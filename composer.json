{
  "name": "nanadjei2/mobiforte",
  "description": "An sms package for mobiforte. http://mobiforte.com",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Adjei Nti Elvis",
      "email": "nana.elvee@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "illuminate/support": "5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0"
  },
  "autoload": {
    "psr-4": {
      "Nanadjei2\\Mobiforte\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nanadjei2\\Mobiforte\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "orchestra/installer": "~3.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Nanadjei2\\Mobiforte\\MobiforteServiceProvider"
      ],
      "aliases": {
        "MobiforteSms": "Nanadjei2\\Mobiforte\\Facades\\MobiforteSms"
      }
    }
  },
  "prefer-stable": true
}
